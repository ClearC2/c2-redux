{
  "name": "c2-redux",
  "version": "1.0.10",
  "description": "an opinionated way to manage redux",
  "repository": {
    "type": "git",
    "url": "https://github.com/ClearC2/c2-redux"
  },
  "main": "lib/index.js",
  "scripts": {
    "start": "cd example && webpack-dev-server --inline --content-base dist --history-api-fallback --port 8088",
    "build": "babel -d lib/ src/",
    "lint": "eslint \"src/**/*.js\"",
    "test": "mocha --require babel-polyfill --require babel-register --require ./test/test_helper.js \"test/**/*.js\"",
    "cm": "git-cz",
    "semantic-release": "semantic-release"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "branch": "master",
    "extends": "c2-semantic-release",
    "npmPublish": true,
    "publish": [
      "@semantic-release/github",
      "@semantic-release/npm"
    ]
  },
  "author": "David Adams",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": "eslint",
    "test/**/*.js": "eslint"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "c2-semantic-release": "ClearC2/c2-semantic-release#^1.0.0",
    "chai": "^4.1.2",
    "chai-immutable": "^2.0.0-alpha.1",
    "css-loader": "^0.28.1",
    "eslint-config-c2-react": "^1.4.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "immutable": "^3.8.2",
    "mocha": "^5.0.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "redux": "^4.0.0",
    "redux-immutable": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "sinon": "^5.0.7",
    "style-loader": "^0.17.0",
    "webpack": "^2.5.0",
    "webpack-dev-server": "^2.4.5"
  },
  "peerDependencies": {
    "immutable": "^3.0.0",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0",
    "react-redux": "^4.0.0 || ^5.0.0",
    "redux": "^3.0.0 || ^4.0.0",
    "redux-immutable": "^4.0.0"
  }
}
